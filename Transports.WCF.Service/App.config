<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  <appSettings>
    <add key="queueName" value=".\private$\transports"/>
  </appSettings>
  <system.serviceModel>
    <services>
      <service name="Transports.WCF.Service.TransportsService">
        <endpoint
           address="net.msmq://localhost/private/transports"
           binding="netMsmqBinding"
           contract="Transports.WCF.Service.ITransportsService">
        </endpoint>
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:6666"/>
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior>
          <serviceMetadata httpGetEnabled="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <bindings>
      <netMsmqBinding>
        <binding>
          <security mode="None"></security>
        </binding>
      </netMsmqBinding>
    </bindings>
  </system.serviceModel>
</configuration>
